-- Helper Functions
local function FormatNumber(n)
    if n >= 1e15 then return string.format("%.2fQ", n / 1e15)
    elseif n >= 1e12 then return string.format("%.2fT", n / 1e12)
    elseif n >= 1e9 then return string.format("%.2fB", n / 1e9)
    elseif n >= 1e6 then return string.format("%.2fM", n / 1e6)
    elseif n >= 1e3 then return string.format("%.2fK", n / 1e3)
    else return tostring(math.floor(n)) end
end

local function UpdateUI()
    MultTitle.Text = string.format("üî• GAIN MULTIPLIER: x%s", FormatNumber(Config.Multiplier))
    SpeedTitle.Text = string.format("‚ö° SPEED MULTIPLIER: x%s", FormatNumber(Config.SpeedMultiplier))
    GainLabel.Text = string.format("üíé Total Gains: +%s", FormatNumber(Stats.TotalGains))
    CountLabel.Text = string.format("üéØ Count: %s", FormatNumber(Stats.TrainCount))
    SpeedLabel.Text = string.format("‚ö° Speed: %s/s", FormatNumber(Stats.GainsPerSecond))
    TimeLabel.Text = string.format("‚è±Ô∏è Session: %ds", math.floor(Stats.SessionTime))
end

-- Input Handlers
MultInput.FocusLost:Connect(function()
    local val = tonumber(MultInput.Text)
    if val and val >= 1 and val <= 9999999 then
        Config.Multiplier = math.floor(val)
        UpdateUI()
    else
        MultInput.Text = tostring(Config.Multiplier)
    end
end)

SpeedInput.FocusLost:Connect(function()
    local val = tonumber(SpeedInput.Text)
    if val and val >= 1 and val <= 10000 then
        Config.SpeedMultiplier = math.floor(val)
        UpdateUI()
    else
        SpeedInput.Text = tostring(Config.SpeedMultiplier)
    end
end)

-- Button Handlers
MinimizeBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
    MinimizedFrame.Visible = true
end)

RestoreBtn.MouseButton1Click:Connect(function()
    MainFrame.Visible = true
    MinimizedFrame.Visible = false
end)

CloseBtn.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
    Config.Enabled = false
    print("üõë Mega Booster Closed")
end)

ActivateBtn.MouseButton1Click:Connect(function()
    Config.Enabled = not Config.Enabled
    if Config.Enabled then
        ActivateBtn.BackgroundColor3 = Color3.fromRGB(50, 255, 50)
        ActivateBtn.Text = "‚úÖ MEGA MODE ACTIVE"
        StatusLabel.Text = "üî• DOMINATING - Gains x" .. FormatNumber(Config.Multiplier)
        StatusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        MinStatus.Text = "üü¢ ON"
        MinStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        ActStroke.Color = Color3.fromRGB(100, 255, 100)
        Stats.SessionTime = 0
    else
        ActivateBtn.BackgroundColor3 = Color3.fromRGB(255, 0, 100)
        ActivateBtn.Text = "üöÄ ACTIVATE MEGA MODE"
        StatusLabel.Text = "‚ö™ STANDBY - Ready to dominate"
        StatusLabel.TextColor3 = Color3.fromRGB(255, 150, 50)
        MinStatus.Text = "‚ö™ OFF"
        MinStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        ActStroke.Color = Color3.fromRGB(255, 100, 150)
    end
end)

AutoBtn.MouseButton1Click:Connect(function()
    Config.AutoTrain = not Config.AutoTrain
    if Config.AutoTrain then
        AutoBtn.BackgroundColor3 = Color3.fromRGB(50, 200, 255)
        AutoBtn.Text = "ü§ñ AUTO TRAIN: ON"
    else
        AutoBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
        AutoBtn.Text = "ü§ñ AUTO TRAIN: OFF"
    end
end)

MultiThreadBtn.MouseButton1Click:Connect(function()
    Config.MultiThread = not Config.MultiThread
