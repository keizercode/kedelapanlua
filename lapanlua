    end
end)

-- MEGA AGGRESSIVE HOOK SYSTEM
local function SetupAggressiveHooks()
    if Config.HookActive then return end
    Config.HookActive = true
    
    -- Hook __namecall - Most aggressive method
    local oldNamecall
    oldNamecall = hookmetamethod(game, "__namecall", function(self, ...)
        local method = getnamecallmethod()
        local args = {...}
        
        if not Config.Enabled then
            return oldNamecall(self, ...)
        end
        
        -- AGGRESSIVE: Hook ALL FireServer calls
        if method == "FireServer" then
            if self.Name == "TrainSpeedHasChanged" then
                if args[1] and tonumber(args[1]) then
                    -- Multiply speed AGGRESSIVELY
                    args[1] = args[1] * Config.SpeedMultiplier
                    Stats.TrainCount = Stats.TrainCount + 1
                end
            end
            
            -- Force multiply ANY numeric argument if aggressive mode
            if Config.AggressiveMode then
                for i, arg in ipairs(args) do
                    if typeof(arg) == "number" and arg > 0 and arg < 1e9 then
                        args[i] = arg * Config.Multiplier
                    end
                end
            end
            
            task.spawn(UpdateUI)
            return oldNamecall(self, unpack(args))
        end
        
        -- AGGRESSIVE: Hook ALL InvokeServer calls
        if method == "InvokeServer" then
            -- Multiply input arguments
            if Config.AggressiveMode then
                for i, arg in ipairs(args) do
                    if typeof(arg) == "number" and arg > 0 and arg < 1e9 then
                        args[i] = arg * Config.Multiplier
                    end
                end
            end
            
            local results = {pcall(oldNamecall, self, unpack(args))}
            local success = table.remove(results, 1)
            
            if success and Config.Enabled then
                -- Multiply ALL numeric returns
                for i, result in ipairs(results) do
                    if typeof(result) == "number" and result > 0 then
                        local gain = result * (Config.Multiplier - 1)
                        Stats.TotalGains = Stats.TotalGains + gain
                        results[i] = result * Config.Multiplier
                    end
                end
                task.spawn(UpdateUI)
            end
            
            return unpack(results)
        end
        
        return oldNamecall(self, ...)
    end)
    
    print("‚úÖ Aggressive Hook Active")
end

-- Hook ALL BindableFunctions AGGRESSIVELY
task.spawn(function()
    task.wait(1)
    
    for _, descendant in pairs(ReplicatedStorage:GetDescendants()) do
        if descendant:IsA("BindableFunction") then
            pcall(function()
                local oldInvoke = descendant.Invoke
                descendant.Invoke = function(self, ...)
                    local results = {oldInvoke(self, ...)}
                    
                    if Config.Enabled then
                        for i, result in ipairs(results) do
                            if typeof(result) == "number" and result > 0 then
                                results[i] = result * Config.Multiplier
                            end
                        end
                    end
                    
                    return unpack(results)
                end
                print("‚úÖ Hooked BindableFunction:", descendant.Name)
            end)
        end
    end
end)

-- Monitor ALL RemoteEvents AGGRESSIVELY
task.spawn(function()
    task.wait(1)
    
    for _, descendant in pairs(ReplicatedStorage:GetDescendants()) do
        if descendant:IsA("RemoteEvent") then
            pcall(function()
                descendant.OnClientEvent:Connect(function(...)
                    if Config.Enabled then
                        local args = {...}
                        for _, arg in ipairs(args) do
                            if typeof(arg) == "number" and arg > 0 then
                                local gain = arg * Config.Multiplier
                                Stats.TotalGains = Stats.TotalGains + gain
                                task.spawn(UpdateUI)
                            end
                        end
                    end
                end)
                print("‚úÖ Monitoring RemoteEvent:", descendant.Name)
            end)
        end
    end
end)

-- MEGA AGGRESSIVE AUTO TRAIN
task.spawn(function()
    while task.wait(0.05) do -- Very fast loop
        if Config.AutoTrain and Config.Enabled then
            pcall(function()
                local TrainEquip = ReplicatedStorage:FindFirstChild("TrainEquipment")
                if TrainEquip and TrainEquip:FindFirstChild("Remote") then
                    
                    if Config.MultiThread then
                        -- Multi-thread spam for MAXIMUM speed
                        for i = 1, 5 do
                            task.spawn(function()
                                local stationary = TrainEquip.Remote:FindFirstChild("ApplyStationaryTrain")
                                if stationary then stationary:InvokeServer() end
                                
                                local mobile = TrainEquip.Remote:FindFirstChild("ApplyMobileTrain")
                                if mobile then mobile:InvokeServer() end
                                
                                local binding = TrainEquip.Remote:FindFirstChild("ApplyBindingTrainingEffect")
                                if binding then binding:InvokeServer("Training_2001", "Emit_2") end
                                
                                local boost = TrainEquip.Remote:FindFirstChild("ApplyBindingTrainingBoostEffect")
                                if boost then boost:InvokeServer() end
                            end)
                        end
                    else
                        -- Single thread
                        local stationary = TrainEquip.Remote:FindFirstChild("ApplyStationaryTrain")
                        if stationary then stationary:InvokeServer() end
                        
                        local mobile = TrainEquip.Remote:FindFirstChild("ApplyMobileTrain")
                        if mobile then mobile:InvokeServer() end
                    end
                end
            end)
        end
    end
end)

-- Ultra fast training speed sender
task.spawn(function()
    while task.wait(0.01) do -- 100 times per second!
        if Config.Enabled and Config.AggressiveMode then
            pcall(function()
                local TrainSystem = ReplicatedStorage:FindFirstChild("TrainSystem")
                if TrainSystem and TrainSystem:FindFirstChild("Remote") then
                    local speedRemote = TrainSystem.Remote:FindFirstChild("TrainSpeedHasChanged")
                    if speedRemote then
                        -- Spam massive speed values
                        speedRemote:FireServer(999999 * Config.SpeedMultiplier)
                    end
                end
            end)
        end
    end
end)

-- Performance monitor
task.spawn(function()
    local lastGains = 0
    while task.wait(1) do
        if Config.Enabled then
            Stats.SessionTime = Stats.SessionTime + 1
            Stats.GainsPerSecond = Stats.TotalGains - lastGains
            lastGains = Stats.TotalGains
            UpdateUI()
        end
    end
end)

-- Animate glow effect
task.spawn(function()
    while task.wait(0.05) do
        if Config.Enabled then
            MainStroke.Color = Color3.fromRGB(
                math.random(200, 255),
                math.random(0, 100),
                math.random(100, 255)
            )
        else
            MainStroke.Color = Color3.fromRGB(255, 0, 100)
        end
    end
end)

-- Initialize
SetupAggressiveHooks()
UpdateUI()

